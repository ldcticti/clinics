<p id="notice"><%= notice %></p>

<table class="table table-striped table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jclinics.each do |jclinic| %>
      <tr>
        <td><%= jclinic.name %></td>
        <td><%= jclinic.phone %></td>
        <td><%= link_to 'Show', jclinic %></td>
        <td><%= link_to 'Edit', edit_jclinic_path(jclinic) %></td>
        <td><%= link_to 'Destroy', jclinic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jclinic', new_jclinic_path, class: "btn btn-secondary" %>
